@article{dussarrat_predictive_nodate,
  title = {Predictive Metabolomics of Multiple {{Atacama}} Plant Species Unveils a Core Set of Generic Metabolites for Extreme Climate Resilience},
  author = {Dussarrat, Thomas and Prigent, Sylvain and Latorre, Claudio and Bernillon, St{\'e}phane and Flandin, Am{\'e}lie and D{\'i}az, Francisca P. and Cassan, C{\'e}dric and Van Delft, Pierre and Jacob, Daniel and Varala, Kranthi and Joubes, J{\'e}r{\^o}me and Gibon, Yves and Rolin, Dominique and Guti{\'e}rrez, Rodrigo A. and P{\'e}triacq, Pierre},
