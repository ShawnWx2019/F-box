@article{ma_resequencing_2018,
  title = {Resequencing a Core Collection of Upland Cotton Identifies Genomic Variation and Loci Influencing Fiber Quality and Yield},
  author = {Ma, Zhiying and He, Shoupu and Wang, Xingfen and Sun, Junling and Zhang, Yan and Zhang, Guiyin and Wu, Liqiang and Li, Zhikun and Liu, Zhihao and Sun, Gaofei and Yan, Yuanyuan and Jia, Yinhua and Yang, Jun and Pan, Zhaoe and Gu, Qishen and Li, Xueyuan and Sun, Zhengwen and Dai, Panhong and Liu, Zhengwen and Gong, Wenfang and Wu, Jinhua and Wang, Mi and Liu, Hengwei and Feng, Keyun and Ke, Huifeng and Wang, Junduo and Lan, Hongyu and Wang, Guoning and Peng, Jun and Wang, Nan and Wang, Liru and Pang, Baoyin and Peng, Zhen and Li, Ruiqiang and Tian, Shilin and Du, Xiongming},
  year = {2018},
