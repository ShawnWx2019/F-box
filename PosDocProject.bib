  author = {Gao, Zhen and Li, Jing and Luo, Meng and Li, Hui and Chen, Qiuju and Wang, Lei and Song, Shiren and Zhao, Liping and Xu, Wenping and Zhang, Caixi and Wang, Shiping and Ma, Chao},
