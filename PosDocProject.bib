@article{jia_genome-wide_2013,
  title = {Genome-Wide Identification and Characterisation of {{F-box}} Family in Maize},
  author = {Jia, Fengjuan and Wu, Bingjiang and Li, Hui and Huang, Jinguang and Zheng, Chengchao},
