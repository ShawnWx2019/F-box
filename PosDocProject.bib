  pmid = {18402703},
  keywords = {/unread,Gene Expression Profiling,Gene Expression Regulation; Plant,Hybrid Vigor,Hybridization; Genetic,Inbreeding,Oligonucleotide Array Sequence Analysis,Zea mays},
