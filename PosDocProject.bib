@article{wang_genome-wide_2019,
  title = {Genome-{{Wide Profiling}} of {{Circular RNAs}} in the {{Rapidly Growing Shoots}} of {{Moso Bamboo}} ({{Phyllostachys}} Edulis)},
  author = {Wang, Yongsheng and Gao, Yubang and Zhang, Hangxiao and Wang, Huihui and Liu, Xuqing and Xu, Xi and Zhang, Zeyu and Kohnen, Markus V. and Hu, Kaiqiang and Wang, Huiyuan and Xi, Feihu and Zhao, Liangzhen and Lin, Chentao and Gu, Lianfeng},
  year = {2019},
