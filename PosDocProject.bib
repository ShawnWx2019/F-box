@article{wu_psrobot_2012,
  title = {{{PsRobot}}: A Web-Based Plant Small {{RNA}} Meta-Analysis Toolbox},
  shorttitle = {{{PsRobot}}},
  author = {Wu, Hua-Jun and Ma, Ying-Ke and Chen, Tong and Wang, Meng and Wang, Xiu-Jie},
