  keywords = {/unread,Genome-wide association studies,Plant breeding},
  annotation = {SC: None[s0]  Bandiera\_abtest: a Cg\_type: Nature Research Journals Primary\_atype: Research Subject\_term: Genome-wide association studies;Plant breeding Subject\_term\_id: genome-wide-association-studies;plant-breeding},
