@article{hu_transcriptomic_2018,
  title = {Transcriptomic Repertoires Depict the Initiation of Lint and Fuzz Fibres in Cotton ({{Gossypium}} Hirsutum {{L}}.)},
  author = {Hu, Haiyan and Wang, Maojun and Ding, Yuanhao and Zhu, Sitao and Zhao, Guannan and Tu, Lili and Zhang, Xianlong},
