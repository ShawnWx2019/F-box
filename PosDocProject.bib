@article{huang_genome_2020,
  title = {Genome Sequence of {{Gossypium}} Herbaceum and Genome Updates of {{Gossypium}} Arboreum and {{Gossypium}} Hirsutum Provide Insights into Cotton {{A-genome}} Evolution},
  author = {Huang, Gai and Wu, Zhiguo and Percy, Richard G. and Bai, Mingzhou and Li, Yang and Frelichowski, James E. and Hu, Jiang and Wang, Kun and Yu, John Z. and Zhu, Yuxian},
  year = {2020},
  month = may,
  journal = {Nature Genetics},
  volume = {52},
  number = {5},
  pages = {516--524},
  publisher = {{Nature Publishing Group}},
  issn = {1546-1718},
  doi = {10.1038/s41588-020-0607-4},
  abstract = {Upon assembling the first Gossypium herbaceum (A1) genome and substantially improving the existing Gossypium arboreum (A2) and Gossypium hirsutum ((AD)1) genomes, we showed that all existing A-genomes may have originated from a common ancestor, referred to here as A0, which was more phylogenetically related to A1 than A2. Further, allotetraploid formation was shown to have preceded the speciation of A1 and A2. Both A-genomes evolved independently, with no ancestor\textendash progeny relationship. Gaussian probability density function analysis indicates that several long-terminal-repeat bursts that occurred from 5.7\,million years ago to less than 0.61\,million years ago contributed compellingly to A-genome size expansion, speciation and evolution. Abundant species-specific structural variations in genic regions changed the expression of many important genes, which may have led to fiber cell improvement in (AD)1. Our findings resolve existing controversial concepts surrounding A-genome origins and provide valuable genomic resources for cotton genetic improvement.},
  copyright = {2020 The Author(s)},
  langid = {english},
  lccn = {38.33},
  keywords = {/unread,Cyclin},
  file = {/Users/shawndoc/Zotero/storage/9S98Y5YY/Huang 等。 - 2020 - Genome sequence of Gossypium herbaceum and genome .pdf;/Users/shawndoc/Zotero/storage/ITDGD98V/41588_2020_607_MOESM3_ESM.xlsx;/Users/shawndoc/Zotero/storage/QYMHCIZQ/zyx-NG-2020Sup.pdf;/Users/shawndoc/Zotero/storage/DHATNRR9/s41588-020-0607-4.html}
